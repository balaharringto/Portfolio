import { Box,Typography} from "@mui/material";
import img1 from "../images/html.png"
import img2 from "../images/css.png"
import img3 from "../images/js.png"
import img4 from "../images/react1.png"
import img5 from "../images/tailwind.png"
import img6 from "../images/python.png"
import img7 from "../images/dj.png"
import img8 from "../images/rest.png"
import img9 from "../images/mysql.png"
import img10 from "../images/mui.png"
import Portfmain from "./Portfmain";

function Porttmain() {
  return (

    <Box
      sx={{
        minHeight: "100vh",
        // background: "linear-gradient(135deg, #0f0f0f, #1a1a1a)",
        color: "white",
        py: 6,
        px: 2,
        fontFamily: "Poppins, sans-serif",
      }}

    >
      <Typography
        variant="h4"
        component="h1"
        sx={{
          color: "#34ebba",
          fontWeight: "bold",
          textAlign: "center",
          mb: 4,
          fontSize: { xs: "28px", md: "40px" },
        }}
      >
        My Skills
      </Typography>

      <Typography
       mt={5}
        variant="h6"
        sx={{
          color: "white",
          textAlign: "center",
          mb: 4,
          fontWeight: "bold",
        }}
      >
        The Skills, Tools and Technologies I Use For Projects
      </Typography>

      <Box mt={5} padding={5}
        sx={{
          display: "flex",
        //   flexWrap: "wrap",
          justifyContent: "center", 
          maxWidth: "1800px",
          mx: "auto",
          marginTop:"30px"
        }}
      >
        {[img1, img2, img3, img4, img5].map((img, idx) => (
          <Box
            key={idx}
            sx={{
              width: { xs: "200px", md: "80px",lg:"200px" },
              height: { xs: "100px", md: "80px",lg:"100px" },
            //   transition: "transform 0.3s ease",
            //   "&:hover": {
            //     transform: "scale(1.2)",
            //   },
            }}
          >
            <img
              src={img}
              alt={`Skill ${idx + 1}`}
              style={{
                width: "100%",
                height: "100%",
                objectFit: "contain",
                // filter: "drop-shadow(0 0 10px #34ebba)",
              }}
            />
          </Box>
        ))}
      </Box>
        <Box
        sx={{
          display: "flex",
          flexWrap: "wrap",
          justifyContent: "center",
          gap: "50px",
          maxWidth: "1200px",
          mx: "auto",
        }}
      >
        {[img6,img7, img8, img9, img10].map((img, idx) => (
          <Box
            key={idx}
            sx={{
              marginTop:"30px",  
              width: { xs: "200px", md: "80px",lg:"110px",xl:"200px" },
              height: { xs: "100px", md: "80px",lg:"100px",xl:"100px" },
            //   transition: "transform 0.3s ease",
            //   "&:hover": {
            //     transform: "scale(1.2)",
            //   },
            }}
          >
            <img
              src={img}
              alt={`Skill ${idx + 1}`}
              style={{
                width: "100%",
                height: "100%",
                objectFit: "contain",
                // filter: "drop-shadow(0 0 10px #34ebba)",
              }}
            />
          </Box>
        ))}
      </Box>
          <Portfmain/>

    </Box>

  );
}

